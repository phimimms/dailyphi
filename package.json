{
  "name": "dailyphi",
  "version": "0.0.0",
  "description": "A day in the life of Philippe Mimms",
  "scripts": {
    "lint":         "node_modules/.bin/esw webpack.config.* src server tools",
    "lint:watch":   "npm run lint -- --watch",
    "open:src":     "babel-node server/server.dev.js",
    "start":        "npm-run-all --parallel open:src lint:watch test:watch",
    "test":         "mocha --reporter spec tools/test-setup.js \"src/**/*.test.js\"",
    "test:watch":   "npm run test -- --watch",
    "clean-dist":   "npm run remove-dist && mkdir dist",
    "remove-dist":  "node_modules/.bin/rimraf ./dist",
    "build:html":   "babel-node tools/buildHtml.js",
    "prebuild":     "npm-run-all clean-dist test lint build:html",
    "build":        "babel-node tools/build.js",
    "postbuild":    "babel-node server/server.prod.js"
  },
  "author": "Philippe Mimms",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.5.0",
    "material-ui": "^0.16.4",
    "mocha": "^3.0.2",
    "mongoose": "^4.7.4",
    "nock": "^8.0.0",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.0",
    "open": "0.0.5",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.1",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  }
}
